/**index.wxss**/
.full-screen {
  background-size: cover;
}

.row {
  padding: 10px;
}

.navigators-wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.navigators-wrap navigator {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 16rpx;
}

.header {
  padding-left: 8px;
  padding-right: 8px;
  display: flex;
  flex-direction: row;
  justify-content: start;
  align-items: center;
}

.body {
  flex: 1;
  padding: 8px;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.footor {
  height: 64px;
  padding: 8px;
}